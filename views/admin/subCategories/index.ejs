<div class="right_col" role="main">
  <div class="">

    <!-- page title starts -->
    <div class="page-title">
      <div class="title_left">
        <!-- <h3>SubCategory</h3> -->
      </div>
    </div>
    <div class="clearfix"></div>
    <!-- page title end -->

    <!-- page content starts -->
    <div class="row">
      <div class="col-md-12 col-sm-12 ">
        <div class="x_panel">
          <div class="x_title">
            <h2>SubCategory<small>list</small></h2>
            <ul class="nav navbar-right panel_toolbox">
              <li> <a href="./subCategories/create" class="btn btn-info btn-sm" rel="tooltip" data-placement="top"
                  title="Add">+ Add SubCategory</a>
              </li>
            </ul>
            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <div class="row">
              <div class="col-sm-12">
                <div class="card-box table-responsive">
                  <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap"
                    cellspacing="0" width="100%">
                    <thead>
                      <tr>
                        <th>S.No</th>
                        <th>Name</th>
                        <th>Main Category</th>
                        <th>Status</th>
                        <th>Action</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% let i=0; for(subCategory of data) { %>
                        <tr>
                          <td>
                            <%=++i%>
                          </td>
                          <td>
                            <%=subCategory.name%>
                          </td>
                          <td>
                            <%=subCategory.category_id.name%>
                          </td>
                          <td>
                            <%= subCategory.status==1 ? 'Active' : 'Deactive' %>
                          </td>
                          <td>
                            <a class="btn btn-info m-1" href="/subCategories/edit/<%=subCategory.id%>"
                              data-toggle="tooltip" rel="tooltip" data-placement="top" title="Edit"><i
                                class="fa fa-edit"></i></a>
                            <a href="javascript:void(0)" class="btn btn-info m-1" onclick="confirm_modal('/subCategories/destroy/<%=subCategory.id%>')"> <i class="fa fa-trash" style="color: #ffffff;"></i></a>

                          </td>
                        </tr>
                        <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- page content end -->
  </div>
</div>